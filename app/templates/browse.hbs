              <button type="button" class="btn btn-color" disabled={{disabled}} {{action 'registeranimalwelfare'}}><h5>Urgent</h5></button>
              <button type="button" class="btn btn-color" disabled={{disabled}} {{action 'registerenvironment'}}><h5>Ending</h5></button>
             
               
   
          

