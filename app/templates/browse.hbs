    {{/if}}
    <!--Animal Welfare-->
  {{#if isAnimalwelfareAuthorize}}
    {{partial 'animal-welfare'}}
    {{/if}}
    <!--Enviorment-->
   {{#if isEnvironmentAuthorize}}
    {{partial 'environment'}}
    {{/if}}
    <!--Film-->
    {{#if isFlimAuthorize}}
    {{partial 'film'}}
    {{/if}}
